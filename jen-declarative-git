pipeline{
    agent any
    
    environment{
        PATH= "/opt/maven36/bin:$PATH"
    }
    stages{
        stage("git checkout"){
            steps{
                git 'git@github.com:jyothsna1991/sparkjava-war-example.git'
            }
        }
        stage("maven build"){
            steps{
                sh "mvn clean package"
            }    
        }
    }
}
